<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <AssemblyVersion>1.1.1.3</AssemblyVersion>
    <FileVersion>1.1.1.3</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="DotNetEnv" Version="2.5.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Binder" Version="7.0.4" />
    <PackageReference Include="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.FileExtensions" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="7.0.0" />
    <PackageReference Include="NLog" Version="5.2.0" />
    <PackageReference Include="Prism.DryIoc" Version="8.1.97" />
    <PackageReference Include="Wada.Data.DesignDepartmentDataBase" Version="1.0.11" />
    <PackageReference Include="Wada.Data.DesignDepartmentDataBase.Models" Version="1.0.15" />
    <PackageReference Include="Wada.Data.OrderManagement" Version="1.1.0-rc-5" />
    <PackageReference Include="Wada.Data.OrderManagement.Models" Version="1.1.0-rc-6" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Wada.AchievementEntry\Wada.AchievementEntry.csproj" />
    <ProjectReference Include="..\Wada.AOP.Logging\Wada.AOP.Logging.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Update=".env">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="NLog.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>